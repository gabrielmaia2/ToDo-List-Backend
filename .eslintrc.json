{
  "env": {
    "es2021": true,
    "node": true
  },
  "extends": ["google"],
  "parser": "@babel/eslint-parser",
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module",
    "babelOptions": {
      "plugins": ["@babel/plugin-syntax-import-assertions"]
    }
  },
  "rules": {
    "new-cap": "off",
    "no-unused-vars": "warn",
    "require-jsdoc": "off"
  }
}
